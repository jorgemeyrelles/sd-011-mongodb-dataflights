{
  "overrides": [
    {
      "files": "*.mongodb",
      "options": {
        "parser": "babel"
      }
    }
  ],
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "insertPragma": false,
  "requirePragma": false,
  "proseWrapPreference": "preserve"
}
